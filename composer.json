{
    "name": "chikenare/openpanel-laravel",
    "description": "Laravel provider for Openpanel",
    "keywords": [
        "openpanel",
        "laravel",
        "analytics",
        "events",
        "tracking"
    ],
    "homepage": "https://github.com/tbleckert/openpanel-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tobias Bleckert",
            "email": "hola@tobiasbleckert.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bleckert\\OpenpanelLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bleckert\\OpenpanelLaravel\\OpenpanelLaravelServiceProvider"
            ]
        }
    }
}
